<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="theme-color" content="#000"/>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>BlackPipe</title>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.4.1/semantic.min.css">
  <link rel="stylesheet" href="/assets/css/style.css">
</head>
<body id="blackpipe-app">
  <div class="ui container">
    <h1 class="ui header grey main-heading">Black Pipe</h1>
    <h4 class="ui header grey session-details">Session ID : <%= sessionId %></h4>
    <!-- <div class="sub header">Session ID : <%= sessionId %></div> -->

    <div class="ui stackable grid">

      <div class="three wide column">
        <div class="ui inverted vertical fluid menu sidebar-menu">
          <div class="item">
            <div class="header">Filter</div>
            <div class="menu" id="filter-attributes">
              <a class="item" data-enabled="0">Critical</a>
              <a class="item" data-enabled="0">Debug</a>
              <a class="item" data-enabled="0">Error</a>
              <a class="item" data-enabled="0">Info</a>
              <a class="item" data-enabled="0">Warning</a>
            </div>
          </div>
          <div class="item">
            <div class="header">Settings</div>
            <div class="menu">
              <a class="item" id="action-clear">Clear</a>
              <a class="item active" id="action-autoscroll">Auto Scroll</a>
            </div>
          </div>
        </div> <!-- menu -->
      </div> <!-- column -->

      <div class="thirteen wide column">
        <pre id="output"></pre>
      </div>
    </div>
  </div>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.2.0/socket.io.js"></script>
  <script>
    let sessionId = '<%= sessionId %>';
  </script>
  <script src="/assets/js/script.js"></script>
</body>
</html>


